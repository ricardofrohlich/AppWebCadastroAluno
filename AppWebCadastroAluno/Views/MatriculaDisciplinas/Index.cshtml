@model IEnumerable<AppWebCadastroAluno.Models.MatriculaDisciplina>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Matricula.Aluno.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Matricula.Aluno.Endereco.Rua)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Disciplina)
            </th>
            <th>
                Curso Matriculado
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Matricula.Aluno.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Matricula.Aluno.Endereco.Rua)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Disciplina.Nome)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Matricula.Curso.Nome)
             </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MatriculaDsciplinaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MatriculaDsciplinaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MatriculaDsciplinaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
