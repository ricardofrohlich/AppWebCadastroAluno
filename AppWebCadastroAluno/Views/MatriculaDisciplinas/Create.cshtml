@model AppWebCadastroAluno.Models.MatriculaDisciplina

@{
    ViewData["Title"] = "Matriculas de disciplinas";
}

<h1>Matricula de disciplinas</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="DisciplinaId" class="control-label"></label>
                <select asp-for="DisciplinaId" class ="form-control" asp-items="ViewBag.DisciplinaId"></select>
            </div>
            <div class="form-group">
                <label asp-for="MatriculaId" class="control-label"></label>
                <select asp-for="MatriculaId" class ="form-control" asp-items="ViewBag.AlunoId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar as matriculas de disciplinas</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
